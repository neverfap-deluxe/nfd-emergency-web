*{font-family:lato,monserrat,noto sans,sans-serif}a{text-decoration:none}body{width:100%;box-sizing:border-box;border:5px solid #000;margin:0!important;background:#fdfdfd}h1{font-size:3rem}h2{font-size:2rem;line-height:3rem;margin-top:.9rem;margin-bottom:.9rem}h3{font-size:1.4rem;line-height:2rem}h4{font-size:1.2rem;line-height:1.8rem}p,b{font-size:1.1rem;line-height:1.6rem}.articles__link{display:inline-block;margin-bottom:1.5rem;transition:border-bottom .1s,color .1s;font-size:1.05rem;line-height:1.6rem;padding:.6rem;border:4px solid #ff2eb6;box-shadow:-5px 5px 0 0 cyan;color:#000!important;transition:box-shadow .2s}.articles__link:hover{color:#ff7bd1;box-shadow:-8px 8px 0 0 #000;border-bottom:4px solid #ff2eb6!important}.link{color:#ff2eb6;text-decoration-line:underline;text-decoration-color:cyan}.link:hover{border-bottom:2px solid cyan}hr{border:0;border-bottom:1px solid #000}.hr2{margin-top:2rem;margin-bottom:1.5rem}.hr2__bottom{margin-top:1.5rem;margin-bottom:2rem}.hr3{margin-top:2rem;margin-bottom:1.5rem}.hr3__bottom{margin-top:1.5rem;margin-bottom:2rem}.hr4{margin-top:2rem;margin-bottom:1.5rem}.hr4__bottom{margin-top:1.5rem;margin-bottom:2rem}.hrul{margin-top:2rem;margin-bottom:1.5rem}.hrul__bottom{margin-top:1.5rem;margin-bottom:2rem}.deluxe__font{font-family:parisienne,cursive}.logo__square{position:relative;right:.2rem;top:-1.3rem;width:14px;height:14px}.em{position:relative;top:-.1rem}.em-innocent{top:0}.em-x,.em-cake,.em-bank{position:relative;left:.2rem;top:0}.em-rainbow{display:none}.em-white_check_mark{position:relative;left:.7rem;width:30px}.em-blush{position:relative;left:.24rem;width:30px}@media screen and (min-width:420px){body{margin:.5rem!important;width:initial}.em-rainbow{display:inline-block}}.contact__form__title-thing:hover{color:#ff61c8}body{position:relative}.emergency__main{position:absolute;top:0;bottom:0;left:0;right:0;display:flex;flex-direction:column;width:100%;height:100%}.emergency__main .emergency__main__top{display:flex}.emergency__main .emergency__main__bottom{display:flex;border-top:4px solid #000}.emergency__main .emergency__main__bottom .emergency__main__bottom__item{border-right:4px solid #000}.emergency__main .emergency__main__bottom .emergency__main__bottom__item .emergency__main__bottom__item--title{color:#fff}.emergency__main .emergency__main__bottom .emergency__main__bottom__item:last-child{border-right:none}.emergency__main__top__left{width:100%;background:#fff}.emergency__main__top__left .emergency__main__top__left--container .emergency__main__top__left--title{color:#fff}.emergency__main__top__right{border-left:4px solid #000;width:200px}